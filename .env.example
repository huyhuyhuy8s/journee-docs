# Next.js Environment Variables
NEXT_PUBLIC_API_URL=http://localhost:5001

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# Liveblocks
NEXT_PUBLIC_LIVEBLOCKS_PUBLISHABLE_KEY=your_liveblocks_publishable_key_here
LIVEBLOCKS_SECRET_KEY=your_liveblocks_secret_key_here

# Font loading settings (to help with Google Fonts issues)
NEXT_PUBLIC_DISABLE_FONT_OPTIMIZATION=true
